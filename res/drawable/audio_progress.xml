<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@*android:id/background">
        <nine-patch android:src="@drawable/audio_seekbar_background" android:dither="true"
          xmlns:android="http://schemas.android.com/apk/res/android" />
    </item>
    <item android:id="@*android:id/secondaryProgress">
        <clip>
            <shape>
                <gradient android:startColor="#80028ac8" android:endColor="#a004638f" android:angle="270.0" android:centerY="0.75" android:centerColor="#80127fb1" />
            </shape>
        </clip>
    </item>
    <item android:id="@*android:id/progress" android:drawable="@drawable/audio_progress_bg" />
</layer-list>